{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.fontFamily": "mono, Roboto Mono, 'Droid Sans Fallback'",
  "editor.fontWeight": "400",
  "workbench.startupEditor": "newUntitledFile",
  "python.jediEnabled": false,
  "editor.minimap.enabled": false,
  "search.smartCase": true,
  "debug.console.fontSize": 12,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "autoDocstring.docstringFormat": "google",
  "workbench.colorCustomizations": {
    "editor.foreground": "#f0f0f0"
  },
  "breadcrumbs.filePath": "off",
  "autoDocstring.includeName": true,
  "workbench.iconTheme": "vscode-seedling-icon-theme",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "diffEditor.ignoreTrimWhitespace": false,
  "C_Cpp.updateChannel": "Insiders",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "editor.cursorSmoothCaretAnimation": true,
  "workbench.enableExperiments": false,
  "C_Cpp.experimentalFeatures": "Enabled",
  "terminal.external.linuxExec": "gnome-terminal",
  "editor.tabCompletion": "on",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.renderWhitespace": "none",
  "python.venvFolders": [
    "venv"
  ],
  "git.confirmSync": false,
  "workbench.sideBar.location": "left",
  "projectManager.groupList": true,
  "python.linting.pydocstyleEnabled": true,
  "workbench.tree.indent": 6,
  "editor.letterSpacing": -0.65,
  "python.linting.pydocstyleArgs": [
    "--ignore=D200,D203,D213,D406,D407"
  ],
  "bashIde.path": "/snap/bin/bash-language-server",
  "window.menuBarVisibility": "toggle",
  "python.languageServer": "Pylance",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "window.zoomLevel": -0.75,
  "workbench.colorTheme": "Atom One Dark",
  "editor.fontSize": 14
}
